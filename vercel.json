{
   "version": 2,
   "name": "music-album",
   "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      {
         "src":"app.py",
         "use":"@vercel/python",
         "config":
         {
            "buildCommand":"pip3 install pickle-mixin"
         }
      }
   ],
   "routes": [
      { "src": "/(.*)", "dest": "/app.js" }
   ]
}