{
    "version": 2,
    "name": "telugu-abusive-comments-detector",
    "buildCommand":"pip3 install pickle-mixin",
    "builds": [
       { "src": "server.js", "use": "@vercel/node && pip3 install pickle-mixin" },
       {"src":"app.py", "use":"@vercel/python && pip3 install pickle-mixin"}
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/server.js" }
    ]
 }